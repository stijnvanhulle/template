{
  "name": "@stijnvanhulle/eslint-config",
  "version": "2.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "module": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./flat": {
      "import": "./dist/flat.js",
      "module": "./dist/flat.js",
      "default": "./dist/flat.cjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ],
      "flat": [
        "./dist/flat.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "schemas",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf ./dist",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint --format pretty .",
    "lint-fix": "bun run lint --quiet --fix",
    "release": "pnpm publish --no-git-check",
    "release:canary": "bash ../../../.github/canary.sh && pnpm publish --no-git-check -tag canary",
    "start": "tsup --watch",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@eslint/js": "^8.45.0",
    "@types/eslint": "~8.40.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "~5.61.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-config-turbo": "~1.10.12",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-react": "~7.33.1",
    "eslint-plugin-unused-imports": "~2.0.0",
    "eslint-plugin-vitest": "~0.2.8",
    "eslint-plugin-vitest-globals": "~1.3.1",
    "globals": "^13.20.0"
  },
  "devDependencies": {
    "@stijnvanhulle/ts-config": "workspace:*",
    "@stijnvanhulle/tsup-config": "workspace:*",
    "eslint": "~8.44.0",
    "tsup": "^7.1.0",
    "typescript": "~5.1.6"
  },
  "packageManager": "pnpm@8.3.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
